<nav class="p-blue">
<%= link_to welcome_index_path do %>
  <%= image_tag('back.png', alt: 'Back', class: "back-icon")%>
  <% end %>
  <p>REGISTER</p>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>

  </nav>

  <%= render "devise/shared/error_messages", resource: resource %>

    
    <div class="field">
        <%= f.text_field :name, placeholder: "Full Name", class: "form-field"  %>
    </div>

    <div class="field">
        <%= f.email_field :email, placeholder: "Email", class: "form-field"  %>
    </div>

    <div class="field">
      <%= f.password_field :password, placeholder: "Password", class: "form-field"  %>
    </div>

    <div class="field">
      <%= f.password_field :password_confirmation, placeholder: "Confirm Password", class: "form-field"  %>
    </div>
    
    <div class="actions bottom-btn p-green">
      <%= f.submit "Next", class: "p-green new-categ-btn"  %>
    </div>


<% end %>

<%= render "devise/shared/links" %>
